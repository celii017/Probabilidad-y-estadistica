        "# Generar datos de la distribucion normal\n",
        "media = 70 # Media\n",
        "desv_std = 4  # Desviacion estandar\n",
        "num_datos = 500\n",
        "# Generamos los datos de manera aleatoria y los ponemo en una lista (array)\n",
        "datos = np.random.normal(media,desv_std,num_datos)\n",
        "datos[0:50] # Mostrar los primeros 50"
